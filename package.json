{
  "name": "@aigniter-site/jumppoint",
  "version": "0.30.5",
  "engines": {
    "node": "^14",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "dev": "blitz dev",
    "dev:watch:tsc": "tsc --watch --noEmit",
    "build": "blitz build",
    "build:stats": "ANALYZE=true yarn run build",
    "start": "blitz start",
    "lint": "eslint --cache --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "echo \"No tests yet\"",
    "postinstall": ""
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "true || tsc && lint-staged && pretty-quick --staged",
      "pre-push": "yarn run lint && yarn run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "blitz": "0.30.5",
    "final-form": "4.20.1",
    "react": "0.0.0-experimental-3310209d0",
    "react-dom": "0.0.0-experimental-3310209d0",
    "react-error-boundary": "3.1.0",
    "react-final-form": "6.5.2",
    "secure-password": "4.0.0",
    "tailwindcss": "2.0.3",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.0.7",
    "@types/react": "17.0.2",
    "@types/secure-password": "3.1.0",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "autoprefixer": "10.2.4",
    "babel-eslint": "10.1.0",
    "eslint": "7.20.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "lint-staged": "10.5.4",
    "postcss": "8.2.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.2.2",
    "webpack-merge": "5.7.3"
  },
  "private": true
}
